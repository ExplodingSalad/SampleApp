<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<databaseChangeLog xmlns="http://www.liquibase.org/xml/ns/dbchangelog" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                   xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-2.0.xsd">

    <changeSet id="add-sample-entity" author="jlo">
        <createTable tableName="SAMPLE">
            <column name="SAMPLE_ID" type="bigint" autoIncrement="true"/>
            <column name="SAMPLE_NAME" type="varchar2"/>
            <column name="SAMPLE_DESCRIPTION" type="varchar2"/>
            <column name="SAMPLE_NUMBER" type="bigint"/>
        </createTable>
    </changeSet>

</databaseChangeLog>